{
  "name": "Vytalitech Light",
  "colors": {
    // Tabs Header Bar (top area where tabs live)
    "editorGroupHeader.tabsBackground": "#bab8b8", // Background behind tabs when tabs are visible
    "editorGroupHeader.noTabsBackground": "#d0d0d0", // Background when no tabs are shown (single file open)

    // Window Title Bar (topmost bar)
    "titleBar.activeForeground": "#000000", // Text color when window is focused
    "titleBar.inactiveForeground": "#444444", // Text color when window is unfocused
    "titleBar.activeBackground": "#b6b6b6", // Background when focused
    "titleBar.inactiveBackground": "#eaeaea", // Background when not focused

    // Breadcrumbs Styling
    "breadcrumb.foreground": "#333333", // Main breadcrumb text
    "breadcrumb.activeSelectionForeground": "#000000", // Currently selected item
    "breadcrumb.focusForeground": "#1a1a1a", // Hovered item
    "breadcrumbPicker.background": "#e0e0e0", // Dropdown background (when you click on breadcrumbs)

    // Main Editor Area
    "editor.background": "#dededc", // Main editor background
    "editor.lineHighlightBackground": "#ffffff", // Highlighted line background color

    // Status Bar (very bottom of window)
    "statusBar.background": "#bab8b8", // Background color of the status bar
    "statusBar.foreground": "#000000", // Text color in the status bar
    "statusBarItem.hoverBackground": "#c3c7d1", // Hover color for items
    "statusBarItem.activeBackground": "#cfcfcf", // Clicked/active background
    "statusBar.noFolderBackground": "#e6e7e9", // Background when no folder is open
    "statusBarItem.prominentBackground": "#cccccc", // Special items (e.g. Git)
    "statusBarItem.prominentHoverBackground": "#bbbbbb", // Hover for special items

    // Side Bar (explorer / extensions view)
    "sideBar.background": "#cecece", // Background color of the sidebar
    "sideBar.foreground": "#000000", // Text color in the sidebar
    "sideBarSectionHeader.foreground": "#000000", // "OPEN EDITORS", "EXPLORER" headers
    "tree.indentGuidesStroke": "#cccccc", // Vertical guide lines color
    "tree.inactiveIndentGuidesStroke": "#ff0000",
    "tree.tableColumnsBorder": "#cccccc", // Optional: list/grid view separator

    // Explorer panel header buttons
    "toolbar.hoverBackground": "#dcdcdc", // Button background on hover
    "toolbar.activeBackground": "#eaeaea", // When pressed or active
    "toolbar.hoverOutline": "#999999", // Border/outline on hover

    // Buttons (New File, New Folder, Collapse, etc.)
    "sideBarTitle.foreground": "#000000", // "EXPLORER" text
    "activityBarBadge.background": "#ff4b4b", // Badge background (e.g. Git changes count)
    "activityBarBadge.foreground": "#ffffff", // Badge text color

    // Tabs (file tabs in editor)
    "tab.activeBackground": "#e8e6e6", // Active tab background
    "tab.inactiveBackground": "#bab8b8", // Inactive tab background
    "tab.border": "#2c2b2b", // Border around tabs
    "tab.activeForeground": "#000000", // Text color of active tab
    "tab.inactiveForeground": "#000000", // Text color of inactive tabs

    // Optional: More fine control for edge cases
    "tab.unfocusedActiveForeground": "#222222", // If active but unfocused
    "tab.unfocusedInactiveForeground": "#666666", // If inactive and unfocused
    "tab.hoverForeground": "#000000", // On hover

    // Dirty (unsaved) file indicator (dot or icon)
    "tab.activeModifiedBorder": "#ff0000",
    "tab.inactiveModifiedBorder": "#ff8888",
    "tab.unfocusedActiveModifiedBorder": "#ffaaaa",
    "tab.unfocusedInactiveModifiedBorder": "#ffcccc",

    // Suggestion / IntelliSense popup (Professional & Informative)
    "editorSuggestWidget.background": "#f5f5f5", // Light gray for soft background
    "editorSuggestWidget.foreground": "#1a1a1a", // Dark gray/black for clean text
    "editorSuggestWidget.selectedBackground": "#e0e0e0", // Subtle highlight for selection
    "editorSuggestWidget.selectedForeground": "#000000", // Solid black for selected text
    "editorSuggestWidget.highlightForeground": "#007acc", // Deep blue for matched text (informative, not alarming)
    "editorSuggestWidget.border": "#cccccc", // Soft gray border

    // Activity Bar (far left column with icons)
    "activityBar.background": "#bab8b8", // Background of activity bar
    "activityBar.foreground": "#000000", // Text/icon color in activity bar

    // Command Palette Background + Input Box
    "quickInput.background": "#d0d0d0", // Main popup background
    "quickInput.foreground": "#1a1a1a", // Text in search bar
    "quickInputTitle.background": "#eaeaea", // Title bar bg (if any)
    "quickInputList.focusForeground": "#007acc",
    "quickInputList.focusBackground": "#d0d0d0",
    "quickInputList.focusIconForeground": "#005bbb",

    // Section Labels inside Command Palette
    "pickerGroup.foreground": "#005bbb", // Group label text ("recently used", etc.)
    "pickerGroup.border": "#cccccc", // Border between groups

    // File currently open (active in the editor)
    "list.activeSelectionForeground": "#000000", // Text color of the active file
    "list.activeSelectionBackground": "#15bcda70", // Background color of the active file

    // File selected (but not active in editor)
    "list.inactiveSelectionBackground": "#15bcda30",

    // Hover effect over file
    "list.hoverForeground": "#000000", // Background on hover
    "list.hoverBackground": "#f5f5f5", // Text on hover

    "focusBorder": "#5cacac",

    // Matched Typed Text
    "list.highlightForeground": "#007acc", // Matched characters (typed filter)

    "tab.activeBorderTop": "#007acc",

    // Checkbox / Multi-Selection Support
    "list.inactiveSelectionForeground": "#333333",
    "list.focusForeground": "#000000",
    "list.focusBackground": "#dcdcdc",

    // Checkmark / selected icon
    "list.focusHighlightForeground": "#007acc", // For match highlight on focused items
    "list.activeSelectionIconForeground": "#005bbb", // Checkmark icon color
    "list.inactiveSelectionIconForeground": "#999999", // Dim checkmark for unfocused

    "editorCursor.foreground": "#007acc", // Cursor color (info blue)
    "editor.selectionBackground": "#cce7ff", // Highlighted selection (light blue)
    "editor.inactiveSelectionBackground": "#d0d0d066", // When editor is unfocused, 40% opacity

    // Bracket Match Highlighting (when your cursor is on a bracket)
    "editorBracketMatch.background": "#00000023",
    "editorBracketMatch.border": "#ff000020",

    // Bracket Pair Colors (6 Levels)
    "editorBracketHighlight.foreground1": "#000000", // Level 1 -Black
    "editorIndentGuide.background1": "#c0bebe",
    "editorIndentGuide.activeBackground1": "#000000",

    "editorBracketHighlight.foreground2": "#000000", // Level 2 - Black
    "editorIndentGuide.background2": "#c0bebe",
    "editorIndentGuide.activeBackground2": "#000000",

    "editorBracketHighlight.foreground3": "#000000", // Level 3 - Black
    "editorIndentGuide.background3": "#c0bebe",
    "editorIndentGuide.activeBackground3": "#000000",

    "editorBracketHighlight.foreground4": "#000000", // Level 4 - Black
    "editorIndentGuide.background4": "#c0bebe",
    "editorIndentGuide.activeBackground4": "#000000",

    "editorBracketHighlight.foreground5": "#000000", // Level 5 - Black
    "editorIndentGuide.background5": "#c0bebe",
    "editorIndentGuide.activeBackground5": "#000000",

    "editorBracketHighlight.foreground6": "#000000", // Level 6 - Black
    "editorIndentGuide.background6": "#c0bebe",
    "editorIndentGuide.activeBackground6": "#000000",

    // Gutter area
    "editorLineNumber.foreground": "#888888",
    "editorLineNumber.activeForeground": "#000000",

    // Unexpected Brackets (like a stray } )
    "editorBracketHighlight.unexpectedBracket.foreground": "#FF0000",

    "editorGhostText.foreground": "#999999", // Soft gray, not harsh

    "diffEditor.insertedTextBackground": "#ddffdd88", // Greenish insert
    "diffEditor.removedTextBackground": "#ffeaea88", // Reddish removal
    "editorGutter.modifiedBackground": "#007acc",
    "editorGutter.addedBackground": "#22863a",
    "editorGutter.deletedBackground": "#d91e18",

    "extensionButton.prominentForeground": "#000000",
    "extensionButton.foreground": "#000000", // Text/icon color on the Install button
    "extensionButton.background": "#e4e8e7", // Button background
    "extensionButton.hoverBackground": "#96e7fc", // Hover background

    // Example: change icon color for buttons in SCM

    "button.background": "#005A9E", // affects commit and other buttons
    "button.foreground": "#ffffff", // icon/text color on buttons
    "icon.foreground": "#005A9E",
    "button.hoverBackground": "#005A9E", // hover effect

    // Panel Header (tabs: Problems, Output, Terminal)
    "panel.background": "#d6d8df", // Background of the entire panel area
    "panel.border": "#b0b0b0", // Border between editor and panel
    "panelTitle.activeForeground": "#000000", // Text color of active tab
    "panelTitle.inactiveForeground": "#464646be", // Text color of inactive tabs
    "panelTitle.activeBorder": "#007acc", // Border under the active tab
    "panelSection.border": "#cccccc", // Divider between terminal tabs and content
    "panelSectionHeader.background": "#eaeaea", // Background behind the terminal tabs
    "panelSectionHeader.foreground": "#000000", // Text/icon color of terminal tabs

    // Terminal Settings
    "terminal.background": "#d6d8df",
    "terminal.foreground": "#000000",
    "terminalCursor.background": "#213132",
    "terminalCursor.foreground": "#03040588",

    "terminal.ansiBlack": "#333333",
    "terminal.ansiRed": "#cc0000",
    "terminal.ansiGreen": "#22863a",
    "terminal.ansiYellow": "#b58900",
    "terminal.ansiBlue": "#0067ce",
    "terminal.ansiMagenta": "#8a2be2",
    "terminal.ansiCyan": "#1ba287",
    "terminal.ansiWhite": "#eaeaea",
    "terminal.ansiBrightBlack": "#666666",
    "terminal.ansiBrightRed": "#e57373",
    "terminal.ansiBrightGreen": "#50b03c",
    "terminal.ansiBrightYellow": "#fdd835",
    "terminal.ansiBrightBlue": "#188cd9",
    "terminal.ansiBrightMagenta": "#d670d6",
    "terminal.ansiBrightCyan": "#4db6ac",
    "terminal.ansiBrightWhite": "#ffffff"
  },
  "tokenColors": [
    {
      "name": "Type & Modifier Declarations",
      "scope": [
        "storage.type.interface.ts",
        "storage.type.namespace.ts",
        "storage.type.enum.ts",
        "storage.type.class.ts",
        "storage.modifier.ts",
        "storage.type.interface.tsx",
        "storage.type.namespace.tsx",
        "storage.type.enum.tsx",
        "storage.type.class.tsx",
        "storage.modifier.tsx",
        "storage.type.interface.js",
        "storage.type.namespace.js",
        "storage.type.enum.js",
        "storage.type.class.js",
        "storage.modifier.js"
      ],
      "settings": {
        "foreground": "#033e6e",
        "fontStyle": "bold"
      }
    },
    {
      "name": "HTML Attribute Names",
      "scope": "entity.other.attribute-name.html",
      "settings": {
        "foreground": "#818c8f",
        "fontStyle": ""
      }
    },
    {
      "name": "HTML Tag Punctuation",
      "scope": [
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html"
      ],
      "settings": {
        "foreground": "#818c8f",
        "fontStyle": ""
      }
    },
    {
      "name": "CSS Property Names",
      "scope": "support.type.property-name.css",
      "settings": {
        "foreground": "#188cd9",
        "fontStyle": ""
      }
    },

    {
      "name": "HTML Text (Derivative)",
      "scope": "text.html.derivative",
      "settings": {
        "foreground": "#000000",
        "fontStyle": ""
      }
    },
    {
      "name": "Class & Type Names",
      "scope": [
        "entity.name.type.class.ts",
        "entity.name.type.class.tsx",
        "entity.name.type.class.js",
        "entity.name.type",
        "entity.name.type.ts",
        "entity.name.type.tsx",
        "support.class",
        "entity.name.type.js"
      ],
      "settings": {
        "foreground": "#0281FF",
        "fontStyle": ""
      }
    },
    {
      "name": "Function & Method Names",
      "scope": ["support.function", "entity.name.function"],
      "settings": {
        "foreground": "#8757c8",
        "fontStyle": ""
      }
    },
    {
      "name": "Variables & Parameters",
      "scope": [
        "variable",
        "variable.parameter.ts",
        "variable.parameter.tsx",
        "variable.parameter.js"
      ],
      "settings": {
        "foreground": "#5E5E5E",
        "fontStyle": ""
      }
    },
    {
      "name": "Generic Strings",
      "scope": "string",
      "settings": {
        "foreground": "#50b03c",
        "fontStyle": ""
      }
    },
    {
      "scope": "Comments",
      "settings": {
        "foreground": "#6a737d",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Type Casts & Type Keywords",
      "scope": [
        "keyword.control.as.ts",
        "entity.name.type.ts",
        "keyword.control.as.tsx",
        "entity.name.type.tsx",
        "keyword.control.as.js",
        "entity.name.type.js"
      ],
      "settings": {
        "foreground": "#8c9156",
        "fontStyle": ""
      }
    },
    {
      "name": "Object Literals & Punctuation",
      "scope": [
        "variable.object.property.ts",
        "meta.object-literal.key.ts",
        "meta.definition.property.ts",
        "meta.field.declaration.ts",
        "meta.brace.round.ts",
        "punctuation.terminator.statement.ts",
        "punctuation.accessor.ts",
        "variable.object.property.tsx",
        "meta.object-literal.key.tsx",
        "meta.definition.property.tsx",
        "meta.field.declaration.tsx",
        "meta.brace.round.tsx",
        "punctuation.terminator.statement.tsx",
        "punctuation.accessor.tsx",
        "punctuation.separator.key-value.html",
        "variable.object.property.js",
        "meta.object-literal.key.js",
        "meta.definition.property.js",
        "meta.field.declaration.js",
        "meta.brace.round.js",
        "punctuation.terminator.statement.js",
        "punctuation.accessor.js"
      ],
      "settings": {
        "foreground": "#000000",
        "fontStyle": ""
      }
    },
    {
      "name": "Property Types",
      "scope": [
        "storage.type.property.ts",
        "storage.type.property.tsx",
        "storage.type.property.js"
      ],
      "settings": {
        "foreground": "#000000",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Import Statements",
      "scope": [
        "keyword.control.import.ts",
        "keyword.control.from.ts",
        "keyword.control.import.tsx",
        "keyword.control.from.tsx",
        "keyword.control.import.js",
        "keyword.control.from.js"
      ],
      "settings": {
        "foreground": "#8E813F",
        "fontStyle": ""
      }
    },
    {
      "name": "Inherited Class Names",
      "scope": [
        "entity.other.inherited-class.ts",
        "entity.other.inherited-class.tsx",
        "entity.other.inherited-class.js"
      ],
      "settings": {
        "foreground": "#4d0045",
        "fontStyle": ""
      }
    },

    {
      "name": "Operators, Annotations, & Control Keywords",
      "scope": [
        "meta.type.annotation.ts",
        "keyword.operator.type.annotation.ts",
        "keyword.operator.optional.ts",
        "support.type.builtin.ts",
        "keyword.operator.type.ts",
        "support.type.primitive.ts",
        "keyword.operator.new.ts",
        "keyword.control.export.ts",
        "keyword.operator.assignment.ts",
        "keyword.control.flow.ts",
        "keyword.operator.comparison.ts",
        "keyword.operator.ternary.ts",
        "meta.type.annotation.tsx",
        "keyword.operator.type.annotation.tsx",
        "keyword.operator.optional.tsx",
        "support.type.builtin.tsx",
        "keyword.operator.type.tsx",
        "support.type.primitive.tsx",
        "keyword.operator.new.tsx",
        "keyword.control.export.tsx",
        "keyword.operator.assignment.tsx",
        "keyword.control.flow.tsx",
        "keyword.operator.comparison.tsx",
        "keyword.operator.ternary.tsx",
        "keyword",
        "meta.type.annotation.js",
        "keyword.operator.type.annotation.js",
        "keyword.operator.optional.js",
        "support.type.builtin.js",
        "keyword.operator.type.js",
        "support.type.primitive.js",
        "support.type.primitive",
        "keyword.operator.new.js",
        "keyword.control.export.js",
        "keyword.operator.assignment.js",
        "keyword.control.flow.js",
        "keyword.operator.comparison.js",
        "keyword.operator.ternary.js"
      ],
      "settings": {
        "foreground": "#8e813f",
        "fontStyle": ""
      }
    },
    {
      "name": "Arithmetic Operators",
      "scope": [
        "keyword.operator.arithmetic.ts",
        "keyword.operator.arithmetic.tsx",
        "keyword.operator.arithmetic.js"
      ],
      "settings": {
        "foreground": "#160eac",
        "fontStyle": ""
      }
    },

    {
      "name": "This, Constants, Readwrite & Object Variables",
      "scope": [
        "variable.language.this.ts",
        "variable.other.property.ts",
        "variable.other.object.property.ts",
        "entity.name.function.ts",
        "variable.other.constant.ts",
        "variable.other.readwrite.ts",
        "variable.other.readwrite.alias.ts",
        "variable.other.object.ts",
        "variable.language.this.tsx",
        "variable.other.property.tsx",
        "variable.other.object.property.tsx",
        "entity.name.function.tsx",
        "variable.other.constant.tsx",
        "variable.other.readwrite.tsx",
        "variable.other.readwrite.alias.tsx",
        "variable.other.object.tsx",
        "variable.language.this.js",
        "variable.other.property.js",
        "variable.other.object.js",
        "variable.other.object.property.js",
        "entity.name.function.js",
        "variable.other.constant.js",
        "variable.other.readwrite.js",
        "variable.other.readwrite.alias.js"
      ],
      "settings": {
        "foreground": "#0067ce",
        "fontStyle": ""
      }
    },
    {
      "name": "JS/TS Embedded HTML Tags",
      "scope": [
        "entity.other.attribute-name.js",
        "punctuation.definition.tag.begin.js",
        "punctuation.definition.tag.end.js",
        "punctuation.definition.tag.begin.ts",
        "punctuation.definition.tag.end.ts",
        "entity.other.attribute-name.ts",
        "punctuation.definition.tag.begin.tsx",
        "punctuation.definition.tag.end.tsx",
        "entity.other.attribute-name.tsx"
      ],
      "settings": {
        "foreground": "#888888",
        "fontStyle": ""
      }
    },

    {
      "name": "Const & Arrow Function",
      "scope": [
        "storage.type.js",
        "storage.type.function.arrow.js",
        "storage.type.ts",
        "storage.type.function.arrow.ts",
        "storage.type.tsx",
        "storage.type.function.arrow.tsx"
      ],
      "settings": {
        "foreground": "#0A3640",
        "fontStyle": "bold"
      }
    },

    {
      "name": "String Quotes & Templates",
      "scope": [
        "string.template.ts",
        "string.quoted.double.ts",
        "string.quoted.single.ts",
        "string.template.tsx",
        "string.quoted.double.tsx",
        "string.quoted.single.tsx",
        "string.quoted.double.js",
        "string.quoted.single.js",
        "string.template.js"
      ],
      "settings": {
        "foreground": "#31ad35",
        "fontStyle": ""
      }
    },
    {
      "name": "Async Modifier",
      "scope": [
        "storage.modifier.async.js",
        "storage.modifier.async.ts",
        "storage.modifier.async.tsx"
      ],
      "settings": {
        "foreground": "#000000",
        "fontStyle": "bold"
      }
    },

    {
      "name": "General Numbers",
      "scope": [
        "constant.numeric",
        "constant.numeric.decimal.js",
        "constant.numeric.decimal.ts",
        "constant.numeric.decimal.tsx"
      ],
      "settings": {
        "foreground": "#DE4A80",
        "fontStyle": ""
      }
    },

    {
      "name": "Boolean, Nulls & Escapes",
      "scope": [
        "constant.language.boolean.true.js",
        "constant.language.null.js",
        "constant.character.escape.js",
        "constant.language.boolean.false.js",
        "constant.language.boolean.true.ts",
        "constant.language.boolean.false.ts",
        "constant.language.null.ts",
        "constant.character.escape.ts",
        "constant.language.boolean.true.tsx",
        "constant.language.boolean.false.tsx",
        "constant.language.null.tsx",
        "constant.character.escape.tsx"
      ],
      "settings": {
        "foreground": "#C95E16",
        "fontStyle": ""
      }
    }
  ]
}
